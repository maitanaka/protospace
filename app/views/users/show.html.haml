.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to user_path(@user) do
          = image_tag @user.avatar, alt: "user_avatar", class: "img media-object", style: "width: 64px; height: 64px;"
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = @user.group
        .row
          .col-md-6
            %h4 Profile
            %p
              = @user.profile
          .col-md-6
            %h4 Works
            %p
              = @user.work
.container.proto-list
  .row
    - @user.prototypes.each do |prototype|
      = render prototype

.text-center
  = paginate(@prototypes)
